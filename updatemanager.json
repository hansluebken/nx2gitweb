    "globalCode": "function formatJSONString(jsonString : text,indentation : number) do\n\tvar result := \"\";\n\tvar level := 0;\n\tlet inQuotes := false;\n\tlet lineBreak := \"\n\";\n\tfor i in range(0, length(jsonString)) do\n\t\tlet char := substring(jsonString, i, i + 1);\n\t\tif char = \"\"\"\" then\n\t\t\tinQuotes := not inQuotes\n\t\tend;\n\t\tif not inQuotes then\n\t\t\tif char = \"{\" or char = \"[\" then\n\t\t\t\tlevel := level + 1;\n\t\t\t\tresult := result + char + lineBreak + lpad(\"\", level * indentation, \" \")\n\t\t\telse\n\t\t\t\tif char = \"}\" or char = \"]\" then\n\t\t\t\t\tlevel := level - 1;\n\t\t\t\t\tresult := result + lineBreak + lpad(\"\", level * indentation, \" \") + char\n\t\t\t\telse\n\t\t\t\t\tif char = \",\" then\n\t\t\t\t\t\tresult := result + char + lineBreak + lpad(\"\", level * indentation, \" \")\n\t\t\t\t\telse\n\t\t\t\t\t\tif char = \":\" then\n\t\t\t\t\t\t\tresult := result + char + \" \"\n\t\t\t\t\t\telse\n\t\t\t\t\t\t\tresult := result + char\n\t\t\t\t\t\tend\n\t\t\t\t\tend\n\t\t\t\tend\n\t\t\tend\n\t\telse\n\t\t\tresult := result + char\n\t\tend\n\tend;\n\tresult\nend;",
